{
  "variables": {
    "year": "2024",
    "resolution": 10,
    "source": "sentinel2_l2a"
  },
  "area": {
    "manager": "eogrow.core.area.CustomGridAreaManager",
    "grid_filename": "grid_slovenia_border_UTM33N_500x500.gpkg",
    "name_column": "identifier"
  },
  "logging": {
    "manager": "eogrow.core.logging.LoggingManager",
    "save_logs": true,
    "show_logs": true,
    "capture_warnings": false,
    "pipeline_logs_backup_interval": 120,
    "stdout_log_packages": [
      "eogrow",
      "__main__"
    ]
  },
  "storage": {
    "manager": "eogrow.core.storage.StorageManager",
    "project_folder": "/media/Obelix/GeoData/slovenia/", //s3://batch-slo-24/",
    "structure": {
      "input_data": "input-data",
      "tiffs": "tiffs/${var:year}",
      "bands": "data/eopatches/${var:source}/${var:year}/bands/",
      "features": "$data/eopatches/${var:source}/{var:year}/features/"
      
    }
  },
  "skip_existing": false
}
